          create_host_path: true
      - type: bind
        source: C:\Users\Никита\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose2425613189016937691_default